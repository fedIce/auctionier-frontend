generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.db"
}


model users {
  uid String @id @default(cuid())
  email String @unique
  token String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}